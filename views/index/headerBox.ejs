<div class="match-header" onclick="toggleDetails('<%= match.id %>')">
    <!-- <tr>
        <td>Team1</td>
        <td>Score1</td>
        <td>Score2</td>
        <td>Team2</td>
        <td>Dur√©e</td>
    </tr> -->

    <div class="timestamp">
        <%= match.timestamp %>
    </div>
    
    <div>
        <div class="scoreWinner">
            <strong>
                <%= match.winner %>
            </strong>
            ( <%= match.teamsData[match.winner]['goals'] %> )
        </div>
        
        <div class="scoreLoser">
            ( <%= match.teamsData[match.loser]['goals'] %> )
            <strong>
                <%= match.loser %>
            </strong>
        </div>
    </div>

    <% if (match.matchResult === "Win") {%>
        <div class="scoreResult WinText">WIN</div>
    <% } else if (match.matchResult === "Lose") { %>
        <div class="scoreResult LoseText">LOSE</div>
    <% } else { %>
        <div class="scoreResult NoNameText">N/A</div>
    <% } %>
    
</div>