<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>RL Game History</title>
    <link rel="stylesheet" href="/CSS/styles.css">
    <link rel="stylesheet" href="/CSS/table.css">
    <link rel="stylesheet" href="/CSS/matchHeader.css">
</head>

<body>
    <header>
        <h1>Rocket League History</h1>
    </header>

    <% if (matches.length===0) { %>
        <p>No match found.</p>
    <% } else { %>
        <% matches.forEach(match=> { %>
            <div class="match-background">
                <%- include('headerBox', { match: match }) %>
                <%- include('matchBox', { match: match }) %>
            </div>
        <% }) %>
    <% } %>
</body>

<script src="client.js"></script>

</html>